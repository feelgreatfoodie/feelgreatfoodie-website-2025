{
  "name": "feelgreatfoodie",
  "version": "1.0.0",
  "description": "FeelGreatFoodie - Where Heritage Meets Flavor. Discover authentic recipes passed down through generations.",
  "private": true,
  "homepage": "https://feelgreatfoodie.com",
  "keywords": [
    "react",
    "recipes",
    "cooking",
    "food",
    "heritage",
    "family-recipes",
    "culinary"
  ],
  "author": {
    "name": "FeelGreatFoodie Team",
    "email": "hello@feelgreatfoodie.com",
    "url": "https://feelgreatfoodie.com"
  },
  "license": "MIT",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "bootstrap": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^3.3.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.0",
    "sass": "^1.63.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,scss,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tsx,json,css,scss,md}",
    "analyze": "npm run build && npx bundle-analyzer build/static/js/*.js",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "test:ci": "CI=true npm test -- --coverage --watchAll=false",
    "serve": "npx serve -s build -l 3000",
    "lighthouse": "npx lighthouse http://localhost:3000 --output html --output-path ./lighthouse-report.html",
    "clean": "rm -rf build node_modules package-lock.json && npm install"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
